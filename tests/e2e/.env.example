TEST_IDENTITY_PRIMARY=YOUR_PRIMARY_IDENTITY_ID
TEST_IDENTITY_SECONDARY=YOUR_SECONDARY_IDENTITY_ID

# Private keys for primary identity
TEST_PRIVATE_KEY_CONTRACT=YOUR_AUTH_KEY_FOR_PRIMARY_IDENTITY
